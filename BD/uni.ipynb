    "    print(e)\n",
    "\n",
    "cur = conn.cursor()\n",
    "cur.execute('SELECT * FROM empleado;')\n",
    "\n",
    "rows = cur.fetchall()\n",
    "for row in rows:\n",
    "    print(row)"
